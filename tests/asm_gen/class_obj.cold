class person{
    private{
        int age;
    };
    public{
        int x;
        int foo()
        {
            printf("You won!\n");
            return 42;
        }
        void bar(int a, int b)
        {
            int c = a + b;
            printf("Kya baat hai!\n");
        }
    };
};


int main()
{	
    class person p;
    class person *ptr;
    p.bar(4, 6);
    (*ptr).foo();
    ptr->bar(4, 6);
    p.x = 88;
    printf("%d\n", p.x);
	return 0;
}
