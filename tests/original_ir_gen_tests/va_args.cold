int find_sum(int count, ...) 
{
    va_list args;
    int sum = 0, i = 0;
    va_start(args, count);
    for (i = 0; i < count; i++)
        sum += va_arg(args, "int");
    va_end(args);
    return sum;
}

int main() {
	int ans = find_sum(3, 2, 4, 6);
    int ans2 = find_sum(2, 1, 3);
    int ans3 = find_sum(4, 1, 2, 3, "4");
    return 0;
}
