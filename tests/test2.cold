
int main() {
    //print decimal to binary
    int decimal;
    int number;
    char binary[32];
    int bit = 0;

    scanf("%d", &decimal);
    number = decimal;
    while(decimal != 0){
        binary[bit++] = (decimal % 2) + '0';
        decimal /= 2;
    }

    printf("Binary representation of %d is:\n",number);
    for (int i = bit - 1; i >= 0; i--) {
        printf("%c", binary[i]);
    }
    printf("\n");
}