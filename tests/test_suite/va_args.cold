
int find_sum(int count, ...) 
{
    va_list args;
    int sum = 0;
    va_start(args, count);
    while(count){
        sum += va_arg(args, "int");
        count -= 1;
    }
    va_end(args);
    return sum;
}

int main() {
	int ans = find_sum(3, 2, 4, 6);
    ans = find_sum(2, 1, 3);
    return 0;
}
