class Person { 
    private{
        int age;
    }; 
    public{ 
        void setAge(int a)  
        { 
            this->age = a; 
        }
        int getAge()  
        { 
            return this->age;
        } 
    };
}; 
  
class Employee <- Person { 
    private{
        int empID;
    };
    public{
        void setEmployeeID(int id)
        { 
            this->empID = id; 
        }
    }; 
}; 
  
int main()  { 
    class Employee emp1;
    int age ;
    emp1.setEmployeeID(101);
    // Expected: empID set to 101

    emp1.setAge(25);
    age = emp1.getAge();

    printf("[main] getAge() returned: %d\n", age);
} 
