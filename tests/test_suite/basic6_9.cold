int switchCase() {
    char op;
    int a, b, c;

    switch (op) {
        case '+':
            c = a + b;
            c = 10;
            break;
        case '-':
            c = b;
            c = 11;
        case '*':
            c = a;
            c = 12;
            break;
        default:
            c = 13;
    }
}

