void test_multi_level_pointers() {
    int val = 10;
    int *ptr = &val;
    int **ptr2 = &ptr;
    int ***ptr3 = &ptr2;
    //
    if (***ptr3 == 10) {
        // Test Passed
    }

    ptr2 = &ptr;
    *ptr = 10; 
    val = *ptr;//both are diff

    ptr = *ptr2;
    **ptr2 = 20;

    *ptr2 = ptr;
    
    //val = *( *(ptr2 + 2) + 1); //TODO : Fix ptr2+2 issue -> use addptr ...
}
