class Person { 
    private{
        int age;
    }; 
    public{ 
        int getAge()  
        { 
            return this->age;
        } 
    };
}; 
  
class Employee <- Person { 
    private{
        int empID;
    };
    public{
        void setEmployeeID(int id)
        { 
            this->empID = id; 
        }
    }; 
}; 
  
int main()  { 
    class Employee emp1;
    emp1.setEmployeeID(101);
    emp1.getAge();
} 
 
