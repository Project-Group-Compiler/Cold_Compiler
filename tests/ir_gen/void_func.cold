class A
{
    public{
        void bar()
        {
            int x = 5;
        }

        void foo(int a)
        {
            int x = 6;
        }
    };
};

void bar()
{
    int x = 5;
}

void foo(int a)
{
    int x = 6;
}

int main()
{
    int b;
    class A a;
    foo(b);
    bar();
    a.foo(b);
    b = 9;
    a.bar();
    b = 9;
    return 0;
}