// IR WORKING

class person{
    public{
        int age;
        void f(){
            (*this).age = 5;
        }
        int g(int x){
            int a = x;
        }
        void foo()
        {
            (*this).f();
            (*this).g(5);
        }
        void bar(int a, int b)
        {
            (*this).f();
            (*this).g(b);
        }
    };
};


int main()
{	
	class person p1;
    class person *ptr;
    int x = 4, y = 6;
    p1.age = 6;
    p1.foo();
    p1.bar(x, y);
    (*ptr).age = 6;
    (*ptr).foo();
    (*ptr).bar(x, y);
	return 0;
}




struct person{
    int age;
};

int main2()
{
    struct person p1;
    struct person *ptr;
    p1.age = 6;
    (*ptr).age = 6;
    return 0;
}