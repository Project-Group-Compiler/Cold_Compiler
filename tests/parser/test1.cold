
int main(){
    /*
        This is a testcase for checking lexer
        .//Special Character: @#$%&   
    */

    inta a, b, c; //checking syntax errors
    char op;
    printf("Enter first number\n");
    scanf("%d",&a);
    printf("Enter second number\n");
    scanf("%d",&b);
    
    printf("Type\n\t'+' for addition\n\t'-' for subtraction\n");
    scanf(" %c",&op);

    switch(op){
        case '+':
            c = a+b;
            printf("%d\n",c);
            break;
        case '-':
            c = a-b;
            printf("%d\n",c);
            break;
        default:
            printf("Not a valid operator\n");
    }
    return 0;
}
