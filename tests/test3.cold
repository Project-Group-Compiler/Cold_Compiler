struct Node {
    int val;
    struct Node* L;
    struct Node* R;
};

struct Node* createNode(int val) {
    struct Node* newNode = (struct Node*)malloc(sizeof(struct Node));
    newNode->val = val;
    newNode->L = NULL;
    newNode->R = NULL;
    return newNode;
}

void inorder(struct Node* root) {
    if (root == NULL) return;
    inorder(root->L);
    printf("%d ", root->val);
    inorder(root->R);
}

int main() {
    struct Node* root = createNode(1);

    root->L = createNode(2);
    root->R = createNode(3);
    root->L->L = createNode(4);
    root->L->R = createNode(5);
    root->R->L = createNode(6);
    root->R->R = createNode(7);

    printf("Inorder Traversal of Binary Tree: ");
    inorder(root);
    printf("\n");

    return 0;
}
